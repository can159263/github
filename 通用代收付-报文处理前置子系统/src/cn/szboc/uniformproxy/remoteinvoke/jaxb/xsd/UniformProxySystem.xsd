			<!-- 处理成功 -->
			<xs:enumeration value="R0000"/>
			<!-- 后台服务不可用 -->
			<xs:enumeration value="R0404"/>
			<!-- 报文格式错 -->
			<xs:enumeration value="R0405"/>
			<!-- 余额不足 -->
			<xs:enumeration value="R1000"/>
			<!-- 账户状态异常 -->
			<xs:enumeration value="R2000"/>
			<!-- 交易窗口关闭 -->
			<xs:enumeration value="R3000"/>
			<!-- 原批量包不存在/原交易不存在 -->
			<xs:enumeration value="R4000"/>
