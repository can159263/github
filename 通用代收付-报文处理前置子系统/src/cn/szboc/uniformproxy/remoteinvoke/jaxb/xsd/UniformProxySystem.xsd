			<xs:enumeration value="R4001"/>
			<!-- 原交易明确失败 -->
			<xs:enumeration value="R5000"/>
			<!-- 原交易已冲正 -->
			<xs:enumeration value="R5001"/>
			<!-- 交易要素非法 -->
			<xs:enumeration value="R6000"/>
			<!-- 账号不存在 -->
			<xs:enumeration value="R6201"/>
			<!-- 账号,户名不符 -->
			<xs:enumeration value="R6202"/>
			<!-- 证件信息不存在 -->
			<xs:enumeration value="R6203"/>
			<!-- 证件类型不匹配 -->
			<xs:enumeration value="R6204"/>
			<!-- 证件号码不匹配 -->
			<xs:enumeration value="R6205"/>
			<!-- 交易失败 -->
			<xs:enumeration value="R8000"/>
			<!-- 银行内部异常 -->
			<xs:enumeration value="R9000"/>
